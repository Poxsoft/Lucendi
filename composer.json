{
    "name": "lucendi/laravel",
    "description": "Poxsoft package for laravel projects",
    "keywords": ["laravel", "lucendi", "poxsoft"],
    "type": "library",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Jose Wilson",
            "email": "jose.dev.web@gmail.com",
            "homepage": "https://wilsapp.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lucendi\\": "src/"
        },
        "files": [
            "./helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Lucendi\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2|~8.0.0|~8.1.0|~8.2.0|~8.3.0",
        "ext-json": "*",
        "laravel/framework": "^10.0|^11.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "orchestra/testbench": "^4.0|^5.0|^6.4|^7.0|^8.0",
        "mockery/mockery": "^1.3.3",
        "phpunit/phpunit": "^8.0|^9.5.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lucendi\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
